{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
        "DB_DIALECT": "@db_dialect",
        "SECRET_KEY": "@secret_key"
      },
    "installCommand": "npm install pg",
    "scripts": {
        "migrate": "sequelize db:migrate",
        "seed": "sequelize db:seed:all"
      }
}